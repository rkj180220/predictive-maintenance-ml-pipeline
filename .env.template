# Environment Variables for Predictive Maintenance Pipeline
# Copy this file to .env and fill in your credentials

# Kaggle API Credentials (Required for dataset download)
# Get these from https://www.kaggle.com/account
# KAGGLE_USERNAME=your_kaggle_username
# KAGGLE_KEY=your_kaggle_api_key

# AWS Configuration (Optional - for CloudWatch integration)
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=predictive-maintenance-data
AWS_CLOUDWATCH_LOG_GROUP=/aws/predictive-maintenance/pipeline
AWS_CLOUDWATCH_LOG_STREAM=pipeline-execution

# Pipeline Configuration
PIPELINE_SCHEDULE_MINUTES=2
LOG_LEVEL=INFO

